
<div class="container">
  <div class="content">
    <div class="row">
      <div class="col-0 col-md-3"></div>
      <div class="col-12 col-md-6">
        <% @articles.each do |article| %>
          <%= link_to article.title, article_path(article) %>
          <p><%= article.content %></p>
        <% end %>

      </div>
    </div>
  </div>
</div>
